{% extends "layout/layout_admin.html.twig" %}

{% block title %}Nouveau Partenaire - {{ parent() }}{% endblock %}

{% block content %}
<div class="container">
    <div class="card">
        <div class="card-content">
            <h1 class="card-title mb-50">Ajouter un nouveau partenaire</h1>
            {{ form_start(form) }}
                {{ form_rest(form) }}
                <button type="submit" class="btn btn-primary">Ajouter le partenaire</button>
            {{ form_end(form) }}
        </div>
    </div>
</div>
{% endblock %}